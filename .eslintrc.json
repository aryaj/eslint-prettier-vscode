{
    "extends":["airbnb","prettier","plugin:node/recommended"], 
    "plugins": ["prettier"],
    "rules": {
        "no-unused-vars":"warn",
        "no-console":"off",
        "prettier/prettier":"error",
        "func-names":"off",
        "prefer-arrow-callback":"error",
        "no-process-exit":"off",
        "object-shorthand":"off"
    }
}
